            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="2dip"/>
                <solid android:color="@color/blinq_background_almost_black_2f2f2f"></solid>
            </shape>
        </clip>
    </item>

</layer-list>

